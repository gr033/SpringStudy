	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="book" class="exam04.BookVO">
		<constructor-arg>
			<value>10</value>
		</constructor-arg>
		
		<constructor-arg>
			<value>자바의 정석</value>
		</constructor-arg>
		
		<constructor-arg>
			<value>어딘지 모를 출판사</value>
		</constructor-arg>
		
		<constructor-arg>
			<value>30000</value>
		</constructor-arg>
	</bean>
</beans>
